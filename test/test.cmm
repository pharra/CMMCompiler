int max(int a,int b){
	if(a>b)
		return a;
	else
		return b;
}
int main(){
	int a;
	int[] ccc = {1,1};
	read(a);
	ccc[0] = 2;
	int c = max(a,7);
	write(c);
	write("\n");
	a = 1;
	for(int i=3;i>0;i=i-1){
		write("for \n");
	}
	while(a<=3){
		write("while \n");
		a=a+1;
	}
}
